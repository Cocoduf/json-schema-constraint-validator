{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"title": "Transaction",
	"type": "object",
	"properties": {
		"largerNumber": {
			"type": "number",
			"minimum": 0
		},
		"smallerNumber": {
			"type": "number",
			"minimum": 0
		},
		"person": {
			"type": "object",
			"properties": {
				"firstName": {
					"type": "string"
				},
				"lastName": {
					"type": "string"
				}
			}
		}
	},
	"required" : ["largerNumber","smallerNumber"]
}
